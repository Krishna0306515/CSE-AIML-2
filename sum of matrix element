#include <iostream>
using namespace std;

int main() {
    int r, c, sum = 0;
    cout << "Enter number of rows and columns: ";
    cin >> r >> c;

    int a[10][10];

    cout << "Enter matrix elements:\n";
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            cin >> a[i][j];
            sum += a[i][j];
        }
    }

    cout << "Sum of matrix elements = " << sum;

    return 0;
}
